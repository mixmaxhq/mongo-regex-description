{
  "name": "mongo-regex-description",
  "version": "1.3.0",
  "description": "This creates a Mongo regex from a \"description\" (see readme for supported descriptions), and vice versa. Useful for building UX on top of regexes.",
  "main": "src/index.js",
  "browser": "dist/browser/index.js",
  "scripts": {
    "build": "[ \"$WATCH\" == 'true' ] && rollup -cw || rollup -c",
    "ci": "npm run lint && npm test",
    "lint": "eslint .",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "JASMINE_CONFIG_PATH='spec/support/jasmine.json' jasmine",
    "watch": "WATCH=true npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixmaxhq/mongo-regex-description.git"
  },
  "keywords": [
    "mongo",
    "sift",
    "regex"
  ],
  "author": "Mixmax <hello@mixmax.com> (https://mixmax.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixmaxhq/mongo-regex-description/issues"
  },
  "homepage": "https://github.com/mixmaxhq/mongo-regex-description#readme",
  "devDependencies": {
    "@mixmaxhq/prettier-config": "^1.0.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^6.8.0",
    "eslint-config-mixmax": "^4.8.0",
    "jasmine": "^2.5.2",
    "prettier": "^1.19.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-watch": "^2.5.0"
  }
}
